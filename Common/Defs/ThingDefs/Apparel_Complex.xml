			<layers>
				<li>OnSkin</li>
			</layers>
			<tags>
				<li>Kurin_Vigilante</li>
				<li>Kurin_Citizen</li>
				<li>Kurin_Complex</li>
				<li>Royal</li>
				<li>RoyalTier2</li>
			</tags>
			<defaultOutfitTags>
				<li>Worker</li>
			</defaultOutfitTags>
			<wornGraphicPath>KurinMeowEdition/Item_Apperal/Complex/TechnicianDress</wornGraphicPath>
		</apparel>
		<statBases>
			<Mass>1</Mass>
			<Flammability>0.8</Flammability>
			<WorkToMake>7500</WorkToMake>
			<MaxHitPoints>150</MaxHitPoints>
