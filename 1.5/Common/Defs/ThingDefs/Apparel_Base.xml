<?xml version="1.0" encoding="utf-8"?>
<Defs>

	<ThingDef Name="KurinApparelBase" ParentName="ApparelBase" Abstract="True">
		<thingClass>Apparel</thingClass>
		<category>Item</category>
		<selectable>True</selectable>
		<pathCost>10</pathCost>
		<useHitPoints>True</useHitPoints>
		<graphicData>
			<onGroundRandomRotateAngle>35</onGroundRandomRotateAngle>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<drawGUIOverlay>true</drawGUIOverlay>
		<apparel>
			<useWornGraphicMask>true</useWornGraphicMask>
			<canBeDesiredForIdeo>false</canBeDesiredForIdeo>
			<shellRenderedBehindHead>true</shellRenderedBehindHead>
		</apparel>
		<tradeability>All</tradeability>
		<tradeTags>
			<li>KurinApparel</li>
		</tradeTags>
		<thingCategories>
			<li>Kurin_Apparel</li>
		</thingCategories>
		<techLevel>Industrial</techLevel>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<Flammability>1.0</Flammability>
			<DeteriorationRate>2</DeteriorationRate>
			<Beauty>-3</Beauty>
		</statBases>
		<altitudeLayer>Item</altitudeLayer>
		<alwaysHaulable>True</alwaysHaulable>
		<tickerType>Never</tickerType>
		<burnableByRecipe>true</burnableByRecipe>
		<smeltable>true</smeltable>
	</ThingDef>

	<ThingDef Name="KurinApparelMakableBase" ParentName="KurinApparelBase" Abstract="True">
		<recipeMaker>
			<workSpeedStat>GeneralLaborSpeed</workSpeedStat>
			<workSkill>Crafting</workSkill>
			<effectWorking>Tailor</effectWorking>
			<soundWorking>Recipe_Tailor</soundWorking>
			<recipeUsers>
				<li>Kurin_TailoringBenchElectric</li>
			</recipeUsers>
			<unfinishedThingDef>UnfinishedApparel</unfinishedThingDef>
			<defaultIngredientFilter>
				<categories>
					<li>Root</li>
				</categories>
				<disallowedThingDefs>
					<li>Gold</li>
					<li>Silver</li>
					<li>Plasteel</li>
					<li>Jade</li>
					<li>Uranium</li>
				</disallowedThingDefs>
			</defaultIngredientFilter>
		</recipeMaker>
	</ThingDef>

	<ThingDef Name="KurinApparelNoColorBase" ParentName="KurinApparelMakableBase" Abstract="True">
		<graphicData>
			<maskPath>dummy256</maskPath>
			<shaderType>CutoutComplex</shaderType>
		</graphicData>
		<comps Inherit="False">
			<li Class="CompProperties_Forbiddable" />
